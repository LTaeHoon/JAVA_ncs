package sec01_Static;
class Student{
	String name,major; //인스턴스 멤버
	int sno; //학번 - 20160001,20160002, ... 2016000n
	static int number = 20160000;
	//인스턴스 초기화 블럭
	{
		++number; //20160001
		sno = number;
	}
}
