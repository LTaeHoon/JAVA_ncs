package sec02_Class;

public class Step04_classType {

	public static void change(int []x,int[]y){//원소 변경
		int temp = 0; //임시변수
		for(int i=0;i<x.length;i++){
			temp = x[i];  //x,y 원소 교체
			x[i] = y[i];
			y[i] = temp;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int []x ={1,2,3};
		int []y ={10,20,30};
		for(int i:x)
			System.out.print(i+" ");
		System.out.println();
		for(int i:y)
			System.out.print(i+" ");
			
		change(x,y); //메서드 호출
		System.out.println(); //빈줄
		for(int i:x)
			System.out.print(i+" ");
		System.out.println();
		for(int i:y)
			System.out.print(i+" ");
			
	}

}
