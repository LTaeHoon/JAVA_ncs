package sec01_Static;
class StaticB{
	//클래스 멤버변수
	static int num1;
	//클래스 멤버메서드
	static void setNum1(int n1){
		num1 = n1;
	}
	void display(){
		System.out.println("num1 ="+ num1);
	}
}
public class Step02_static_method {
	/*
	 * 클래스 멤버 메서드 
	 *  - 클래스 멤버 변수 초기화 목적
	 *  - 형식) static 메서드명(){  }
	 *  - 주의 : 인스턴스 멤버변수 사용 불가
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StaticB.setNum1(10);
	}

}
