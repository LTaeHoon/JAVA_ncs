package sec05_Inner;

interface InterA{
	int sum(int x,int y);
	
}
public class Step02_lambda {

	public static void main(String[] args) {
		// jdk1.7 방식 - 무명클래스 객체 생성
		InterA in1 = new InterA(){

			@Override
			public int sum(int x, int y) {
				// TODO Auto-generated method stub
				return x+y;
			}
			
		};
	}

}
