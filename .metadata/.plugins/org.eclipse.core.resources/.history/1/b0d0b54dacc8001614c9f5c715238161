package sec02_inheritance;
class Car{  //부모클래스
	int cc; //엔진
	int door; //문짝
	Car(){ } //기본 생성자
	
	
}
public class Step01_inher {
	/*
	 * 상속(Inheritance) ?
	 *  - 기존의 클래스를 재사용하여 새로운 클래스를 생성하는 문법
	 *  - 부모클래스의 모든 멤버를 상속받는다.
	 *    -> 단, 생성자와 초기화 블럭 상속 제외
	 *  - 자식클래스는 부모 멤버보다 같거나 크다.  
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
