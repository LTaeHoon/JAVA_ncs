package sec01_Static;
class StaticD{
	//PI 변수의 상수화
	static final double PI = 3.14159;

}
public class Step05_static_final {
	/*
	 * 변수의 상수화
	 *  - 변수를 상수처럼 생성
	 *  - 변수명을 대문자로 표시
	 *  - static final 변수 = 초기화;
	 *  - PI = 3.14159
	 */
	public static void main(String[] args) {
		// PI 리턴
		double pi = StaticD.PI;
		System.out.println("파이 = "+ PI);
		
	}

}
