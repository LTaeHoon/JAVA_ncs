package sec02_Class;

class B{
	int num; //멤버변수
	B(){}    //기본 생성자 : 객체만 생성하는 역할
	
	B(int n){ //생성자 : 객체 생성 + 멤버변수 초기화
		num = n; //멤버변수 초기화
	}
	
}
public class Step08_const2 {
	
	/*
	 * 생성자 다중정의
	 *  - 하나의 클래스에 다수의 생성자 정의 문법
	 *  - 주의 : 매개변수의 자료형 또는 개수는 달라야 함
	 */
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
