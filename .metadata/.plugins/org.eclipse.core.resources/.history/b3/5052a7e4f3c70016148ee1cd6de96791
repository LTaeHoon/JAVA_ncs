package chap06_class;
class Rect{
	int w,h;
	Rect(){}
	Rect(int w, int h){
		this.w=w;
		this.h=h;
	}
	int area(){
		return w*h;
	}
	int circum(){
		return 2*(w+h);
	}
}
public class class01 {
/*
 * 멤버 변수 : 가로(w), 세로(h)
생성자 : 가로, 세로 멤버변수 초기화
멤버 메서드(area) : 사각형의 넓이를 구하는 메서드
멤버 메서드(circum) : 사각형의 둘레를 구하는 메서드
 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Rect r = new Rect()
	}

}
