package sec02_Class;

class Account{ //계좌클래스
	private int balance; //잔액 (0)
	
	void setBalance(int b){ //Setter
		balance = b;
	}
	int getBalance(){  //Getter
		return balance;
	}
	
	
}
public class Step06_account {
	/*
	 * 계좌관련 클래스 정의
	 */
	public static void main(String[] args) {
		// Account 객체 생성
		Account ac = new Account();
		
		
	}

}
